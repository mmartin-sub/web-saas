{
  "name": "@saasfly/nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "contentlayer2 build && bun with-env:dev next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "bun with-env:dev next dev",
    "lint": " bun with-env:dev next lint --quiet",
    "format": "prettier --write '**/*.{js,cjs,mjs,ts,tsx,md,json}'  --ignore-path .prettierignore",
    "start": "bun with-env:dev next start",
    "with-env:dev": "dotenv -e ../../.env.development -e ../../.env.local --",
    "with-env:prod": "dotenv -e ../../.env.production -e ../../.env.local --"
  },
  "dependencies": {
    "@dinero.js/currencies": "2.0.0-alpha.14",
